    "@auth0/angular-jwt": "^5.2.0",
    "@microsoft/signalr": "^8.0.7",
    "@ngrx/store": "^18.0.2",
    "@nx/eslint-plugin": "^21.5.3",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.11",
