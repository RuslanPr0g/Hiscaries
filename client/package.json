    "nx": "21.5.1",
    "postcss": "^8.4.5",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.0.2",
    "ts-jest": "^29.4.4",
    "ts-node": "10.9.1",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.16.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix --ignore-pattern \".angular/*\"",
      "prettier --write"
