{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "useraccounts": {
        "ClusterId": "useraccounts-cluster",
        "Match": {
          "Path": "/api/v1/accounts/{**catch-all}"
        }
      },
      "notifications": {
        "ClusterId": "notifications-cluster",
        "Match": {
          "Path": "/api/v1/notifications/{**catch-all}"
        }
      },
      "platformusers": {
        "ClusterId": "platformusers-cluster",
        "Match": {
          "Path": "/api/v1/users/{**catch-all}"
        }
      },
      "stories": {
        "ClusterId": "stories-cluster",
        "Match": {
          "Path": "/api/v1/stories/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "useraccounts-cluster": {
        "Destinations": {
          "useraccounts-destination": {
            "Address": "https://hc-useraccounts-api-rest/"
          }
        }
      },
      "notifications-cluster": {
        "Destinations": {
          "notifications-destination": {
            "Address": "https://hc-notifications-api-rest/"
          }
        }
      },
      "platformusers-cluster": {
        "Destinations": {
          "platformusers-destination": {
            "Address": "https://hc-platformusers-api-rest/"
          }
        }
      },
      "stories-cluster": {
        "Destinations": {
          "stories-destination": {
            "Address": "https://hc-stories-api-rest/"
          }
        }
      }
    }
  }
}